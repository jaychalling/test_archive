{
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "WebFetch(domain:docs.google.com)",
      "Bash(npm run build:*)",
      "Bash(npm run dev:*)",
      "Bash(curl:*)",
      "Bash(taskkill:*)",
      "Bash(cmd.exe /c \"taskkill /PID 34280 /F\")",
      "Bash(netstat:*)",
      "Bash(powershell.exe -Command \"Stop-Process -Id 34280 -Force\")",
      "Bash(git push:*)",
      "Bash(mkdir:*)"
    ]
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python \"$CLAUDE_PROJECT_DIR/.claude/hooks/route-to-agent.py\"",
            "statusMessage": "Checking for agent suggestions..."
          }
        ]
      }
    ]
  }
}
